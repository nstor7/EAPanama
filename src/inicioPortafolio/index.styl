.inicioPortafolio
  position relative
  display: inline-block
  height: inherit
  .portafolioTitulo
    width: 100%
    margin-top: 40px
    text-align: center
    .portafolioIcon
      width: 68px
      height: 68px
      background: #e1e1e1
      margin: auto
    h2
      font-size: 54px
      color: #333
      margin: 0 0 5px 0
    p
      font-family: 'Montserrat', sans-serif
      color: #666
      padding: 0
  .portafolioProyectos
    display flex
    justify-content: space-around
    flex-wrap wrap
    padding 0 20px
    div
      width: 245px
      height: 345px
      background: #e1e1e1
      margin-bottom: 20px
  .masProyectos
    width: 225px
    height: 46px
    display: block
    border-radius: 5px
    background: #c8db4d
    margin: 20px auto 20px
    font-size: 14px
    text-transform: uppercase
    line-height: 46px
    color: #fff
    text-decoration: none
    letter-spacing: 1px
    box-shadow: 0 -3px rgba(0,0,0,0.3) inset
    font-family: 'Montserrat', sans-serif
    text-align: center
  .masProyectos:hover
    background: #69645e
