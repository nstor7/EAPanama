section
  height 100vh
  width: 100%
  display flex
  justify-content: space-around
  background: white
.objetivo
  position absolute
  top 100vh
  z-index: 1
  justify-content: center

  .objetivoTexto
    width: 40%
    margin-top 80px
    h2
      font-size: 44px
      font-weight: bold
      margin: 0
      span
        color #C8DB4D
    p
      font-family: 'Montserrat', sans-serif
      line-height: 1.6
      text-align: justify
      color #666
    p:last-child
      text-align: center
  .imagenObjetivo
    width: 454px
    height: 515px
    background: #e1e1e1
    margin 80px 0 0 80px
